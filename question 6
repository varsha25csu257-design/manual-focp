#include <iostream>
using namespace std;

int main() {
    int n;
    float basic, bonus, net_salary;

    // Taking number of employees
    cout << "Enter number of employees: ";
    cin >> n;

    for (int i = 1; i <= n; i++) {
        cout << "\nEnter basic salary of employee " << i << ": ";
        cin >> basic;

        // Calculating bonus (12%)
        bonus = basic * 0.12;

        // Calculating net salary
        net_salary = basic + bonus;

        // Displaying results
        cout << "Bonus (12%) = " << bonus << endl;
        cout << "Net Salary = " << net_salary << endl;
    }

    return 0;
}
